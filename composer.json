{
    "name": "msgphp/user",
    "type": "library",
    "description": "A domain layer providing basic user management",
    "keywords": [
        "ddd",
        "domain-driven-design",
        "rich domain model",
        "user",
        "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "msgphp/domain": "^0.15"
    },
    "require-dev": {
        "doctrine/orm": "^2.13",
        "symfony/form": "^5.4 || ^6.4",
        "symfony/messenger": "^5.4 || ^6.4",
        "symfony/security-core": "^5.4 || ^6.4",
        "symfony/validator": "^5.4 || ^6.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.15-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\User\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
